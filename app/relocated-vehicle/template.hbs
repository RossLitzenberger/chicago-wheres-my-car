<div class="panel panel-default">
    <div class="panel-heading text-center"><h2>This vehicle was relocated</h2></div>
    <div class="panel-body">
        <table class="table">
            <tbody>
                <tr>
                    <td>Service Request Number</td>
                    <td>{{model.id}}</td>
                </tr>
                <tr>
                    <td>Date Relocated</td>
                    <td>{{moment-calendar model.date}}</td>
                </tr>
                <tr>
                    <td>Description</td>
                    <td>{{model.description}}</td>
                </tr>
                <tr>
                    <td>License Plate</td>
                    <td>{{model.licensePlate}}</td>
                </tr>
                <tr>
                    <td>Reason for Relocation</td>
                    <td>{{model.relocatedReason}}</td>
                </tr>
                <tr>
                    <td>Moved from</td>
                    <td>{{model.relocatedFrom}}</td>
                </tr>
                <tr>
                    <td>Moved to</td>
                    <td>{{model.relocatedTo}}</td>
                </tr>
            </tbody>
        </table>
        <div class="text-center">
            <a
                href="https://www.google.com/maps/dir/{{model.relocatedFrom}}/{{model.relocatedTo}}"
                target="_blank"
            >
                See map
            </a>
        </div>
    </div>
</div>
